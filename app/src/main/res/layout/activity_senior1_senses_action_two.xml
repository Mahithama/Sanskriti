        <ImageView
            android:id="@+id/ride_c_n"
            android:layout_width="94dp"
            android:layout_height="90dp"
            android:layout_marginTop="20dp"
            android:visibility="invisible"
            android:layout_marginLeft="320dp"
            app:srcCompat="@drawable/ride_bdp" />


        <TextView
            android:id="@+id/kneee"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="85dp"
            android:layout_marginLeft="246dp"
            android:fontFamily="@font/primer_print_bold"
            android:text="Knee"
            android:textColor="#000"
            android:textSize="25dp" />

        <ImageView
            android:id="@+id/hand"
            android:layout_width="182dp"
            android:layout_height="121dp"
            android:layout_marginLeft="450dp"
            app:srcCompat="@drawable/hand_bdp" />

        <ImageView
            android:id="@+id/write_c"
            android:layout_width="182dp"
            android:layout_height="100dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="450dp"
            app:srcCompat="@drawable/dummy" />

        <ImageView
            android:id="@+id/write_c_n"
            android:layout_width="94dp"
            android:layout_height="90dp"
            android:layout_marginTop="20dp"
            android:visibility="invisible"
            android:layout_marginLeft="540dp"
            app:srcCompat="@drawable/write_bdp" />


        <TextView
            android:id="@+id/handd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="85dp"
            android:layout_marginLeft="470dp"
            android:fontFamily="@font/primer_print_bold"
            android:text="Hand"
            android:textColor="#000"
            android:textSize="25dp" />


    </FrameLayout>
    <FrameLayout
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:layout_width="wrap_content"
       android:layout_alignParentBottom="true"
        android:layout_height="wrap_content"
        android:layout_below="@+id/layout_2">

    <ImageView
        android:id="@+id/drag_run"
        android:layout_width="94dp"
        android:layout_height="90dp"
        app:srcCompat="@drawable/run_bdp" />

    <ImageView
        android:layout_marginLeft="100dp"
        android:id="@+id/drag_kick"
        android:layout_width="94dp"
        android:layout_height="90dp"
        app:srcCompat="@drawable/kick_bdp" />

    <ImageView
        android:layout_marginLeft="200dp"
        android:id="@+id/drag_taste"
        android:layout_width="94dp"
        android:layout_height="90dp"
        app:srcCompat="@drawable/taste_bdp" />

    <ImageView
        android:layout_marginLeft="300dp"
        android:id="@+id/drag_bite"
        android:layout_width="94dp"
        android:layout_height="90dp"
        app:srcCompat="@drawable/bite_bdp" />

    <ImageView
        android:layout_marginLeft="400dp"
        android:id="@+id/drag_write"
        android:layout_width="94dp"
        android:layout_height="90dp"
        app:srcCompat="@drawable/write_bdp" />

    <ImageView
        android:layout_marginLeft="500dp"
        android:id="@+id/drag_ride"
        android:layout_width="94dp"
        android:layout_height="90dp"
        app:srcCompat="@drawable/ride_bdp" />
    </FrameLayout>
</RelativeLayout>