<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:background="@drawable/unit_one_themes_bg"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="101dp">


        <ImageView
            android:id="@+id/stories_jbut"
            android:layout_width="160dp"
            android:layout_height="160dp"
            android:layout_marginLeft="10dp"
            android:background="@drawable/stori_but"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.324" />


        <ImageView
            android:id="@+id/act_jbut"
            android:layout_width="160dp"
            android:layout_height="160dp"
            android:layout_marginLeft="190dp"
            android:background="@drawable/acti_but"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.503"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.595" />


    </FrameLayout>
    <androidx.appcompat.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:background="@color/sans_nav" />



    <ImageView
        android:id="@+id/logout_icon"
        android:layout_width="50dp"
        android:layout_alignParentBottom="true"
        android:layout_height="59dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:src="@drawable/kg_logout_icon" />

    <ImageView
        android:id="@+id/home_icon"
        android:layout_alignParentBottom="true"
        android:layout_width="50dp"
        android:layout_height="59dp"
        android:layout_centerInParent="true"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:src="@drawable/kg_home_icon" />

    <ImageView
        android:id="@+id/back_icon"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="10dp"
        android:layout_width="50dp"
        android:layout_height="35dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_centerInParent="true"
        android:layout_marginRight="20dp"
        android:src="@drawable/kg_back_icon" />


</RelativeLayout>